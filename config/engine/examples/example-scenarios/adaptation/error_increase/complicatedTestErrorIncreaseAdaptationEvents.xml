<!-- SCENARIO FOR TESTING THE INCREMENTAL OF WEATHER PREDICTION ERROR-->
<ScenarioEventsArgs>
	<event type="NEW_CLIENT_EVENT">
		<occurrenceTime>2</occurrenceTime>
		<name>Client1</name>
		<jobId>1</jobId>
		<start>2</start>
		<end>5</end>
		<deadline>15</deadline>
		<power>60</power>
	</event>
	<!-- EXPECTED OUTCOME: JOB IS SPLIT AND ACCEPTED BY BOTH GS	-->
	<event type="NEW_CLIENT_EVENT">
		<occurrenceTime>3</occurrenceTime>
		<name>Client2</name>
		<jobId>2</jobId>
		<start>2</start>
		<end>4</end>
		<deadline>15</deadline>
		<power>10</power>
	</event>
	<!-- EXPECTED OUTCOME: JOB ACCEPTED BY WINDFARM1 -->
	<event type="NEW_CLIENT_EVENT">
		<occurrenceTime>4</occurrenceTime>
		<name>Client3</name>
		<jobId>3</jobId>
		<start>1</start>
		<end>4</end>
		<deadline>4</deadline>
		<power>30</power>
	</event>
	<!-- EXPECTED OUTCOME: JOB FAILED, POWER SUCCESS RATIO DROP	-->
	<event type="POWER_SHORTAGE_EVENT">
		<occurrenceTime>8</occurrenceTime>
		<agentName>WindFarm1</agentName>
		<newMaximumCapacity>0</newMaximumCapacity>
		<isFinished>false</isFinished>
		<cause>WEATHER_CAUSE</cause>
	</event>
	<!-- EXPECTED OUTCOME: WINDFARM1 SELECTED FOR ADAPTATION -->
	<!-- SYSTEM STATE: FINISHED EXECUTING ALL JOBS -->
	<event type="NEW_CLIENT_EVENT">
		<occurrenceTime>10</occurrenceTime>
		<name>Client4</name>
		<jobId>4</jobId>
		<start>1</start>
		<end>8</end>
		<deadline>30</deadline>
		<power>10</power>
	</event>
	<!-- EXPECTED OUTCOME: JOB ACCEPTED BY WINDFARM2 DUE TO INCREASED ERROR IN WINDFARM1 -->
</ScenarioEventsArgs>